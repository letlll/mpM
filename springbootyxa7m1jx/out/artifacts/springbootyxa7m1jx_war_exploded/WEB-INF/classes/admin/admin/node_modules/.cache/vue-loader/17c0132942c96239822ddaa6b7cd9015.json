laceholder=\"受助人\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"联系方式\" prop=\"lianxifangshi\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxifangshi\" placeholder=\"联系方式\" clearable  :readonly=\"ro.lianxifangshi\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"联系方式\" prop=\"lianxifangshi\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxifangshi\" placeholder=\"联系方式\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-if=\"type!='info' && !ro.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<file-upload\r\n\t\t\t\t\t\ttip=\"点击上传头像\"\r\n\t\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t\t:limit=\"3\"\r\n\t\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t\t:fileUrls=\"ruleForm.touxiang?ruleForm.touxiang:''\"\r\n\t\t\t\t\t\t@change=\"touxiangUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-else-if=\"ruleForm.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<img v-if=\"ruleForm.touxiang.substring(0,4)=='http'&&ruleForm.touxiang.split(',w').length>1\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else-if=\"ruleForm.touxiang.substring(0,4)=='http'\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang.split(',')[0]\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.touxiang.split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"分类\" prop=\"fenlei\" >\r\n\t\t\t\t\t<el-select :disabled=\"ro.fenlei\" v-model=\"ruleForm.fenlei\" placeholder=\"请选择分类\" >\r\n\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\tv-for=\"(item,index) in fenleiOptions\"\r\n\t\t\t\t\t\t\tv-bind:key=\"index\"\r\n\t\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"分类\" prop=\"fenlei\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.fenlei\"\r\n\t\t\t\t\t\tplaceholder=\"分类\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t提交\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn4\" v-if=\"type!='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t取消\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn5\" v-if=\"type=='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t返回\r\n\t\t\t\t</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n    \r\n\r\n\t</div>\r\n</template>\r\n<script>\r\n\timport { \r\n\t\tisMobile,\r\n\t} from \"@/utils/validate\";\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\tvar validateMobile = (rule, value, callback) => {\r\n\t\t\t\tif(!value){\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t} else if (!isMobile(value)) {\r\n\t\t\t\t\tcallback(new Error(\"请输入正确的手机号码\"));\r\n\t\t\t\t} else {\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\treturn {\r\n\t\t\t\tid: '',\r\n\t\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\t\tro:{\r\n\t\t\t\t\tshouzhuzhanghao : false,\r\n\t\t\t\t\tmima : false,\r\n\t\t\t\t\tshouzhuren : false,\r\n\t\t\t\t\tlianxifangshi : false,\r\n\t\t\t\t\ttouxiang : false,\r\n\t\t\t\t\tfenlei : false,\r\n\t\t\t\t},\r\n\t\t\t\r\n\t\t\t\truleForm: {\r\n\t\t\t\t\tshouzhuzhanghao: '',\r\n\t\t\t\t\tmima: '',\r\n\t\t\t\t\tshouzhuren: '',\r\n\t\t\t\t\tlianxifangshi: '',\r\n\t\t\t\t\ttouxiang: '',\r\n\t\t\t\t\tfenlei: '',\r\n\t\t\t\t},\r\n\t\t\t\tfenleiOptions: [],\r\n\r\n\t\t\t\trules: {\r\n\t\t\t\t\tshouzhuzhanghao: [\r\n\t\t\t\t\t\t{ required: true, message: '受助账号不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tmima: [\r\n\t\t\t\t\t\t{ required: true, message: '密码不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tshouzhuren: [\r\n\t\t\t\t\t\t{ required: true, message: '受助人不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tlianxifangshi: [\r\n\t\t\t\t\t\t{ validator: validateMobile, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\ttouxiang: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tfenlei: [\r\n\t\t\t\t\t\t{ required: true, message: '分类不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t},\r\n\t\t\t};\r\n\t\t},\r\n\t\tprops: [\"parent\"],\r\n\t\tcomputed: {\r\n\r\n\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t// 下载\r\n\t\t\tdownload(file){\r\n\t\t\t\twindow.open(`${file}`)\r\n\t\t\t},\r\n\t\t\t// 初始化\r\n\t\t\tinit(id,type) {\r\n\t\t\t\tif (id) {\r\n\t\t\t\t\tthis.id = id;\r\n\t\t\t\t\tthis.type = type;\r\n\t\t\t\t}\r\n\t\t\t\tif(this.type=='info'||this.type=='else'||this.type=='msg'){\r\n\t\t\t\t\tthis.info(id);\r\n\t\t\t\t}else if(this.type=='logistics'){\r\n\t\t\t\t\tfor(let x in this.ro) {\r\n\t\t\t\t\t\tthis.ro[x] = true\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.logistics=false;\r\n\t\t\t\t\tthis.info(id);\r\n\t\t\t\t}else if(this.type=='cross'){\r\n\t\t\t\t\tvar obj = this.$storage.getObj('crossObj');\r\n\t\t\t\t\tfor (var o in obj){\r\n\t\t\t\t\t\tif(o=='shouzhuzhanghao'){\r\n\t\t\t\t\t\t\tthis.ruleForm.shouzhuzhanghao = obj[o];\r\n\t\t\t\t\t\t\tthis.ro.shouzhuzhanghao = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif(o=='mima'){\r\n\t\t\t\t\t\t\tthis.ruleForm.mima = obj[o];\r\n\t\t\t\t\t\t\tthis.ro.mima = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif(o=='shouzhuren'){\r\n\t\t\t\t\t\t\tthis.ruleForm.shouz