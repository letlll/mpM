el-input v-model=\"ruleForm.shouzhuren\" placeholder=\"受助人\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"联系方式\" prop=\"lianxifangshi\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxifangshi\" placeholder=\"联系方式\" clearable  :readonly=\"ro.lianxifangshi\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"联系方式\" prop=\"lianxifangshi\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxifangshi\" placeholder=\"联系方式\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"date\" v-if=\"type!='info'\" label=\"更新时间\" prop=\"gengxinshijian\" >\r\n\t\t\t\t\t<el-date-picker\r\n\t\t\t\t\t\tvalue-format=\"yyyy-MM-dd HH:mm:ss\"\r\n\t\t\t\t\t\tv-model=\"ruleForm.gengxinshijian\" \r\n\t\t\t\t\t\ttype=\"datetime\"\r\n\t\t\t\t\t\t:readonly=\"ro.gengxinshijian\"\r\n\t\t\t\t\t\tplaceholder=\"更新时间\"\r\n\t\t\t\t\t></el-date-picker>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-else-if=\"ruleForm.gengxinshijian\" label=\"更新时间\" prop=\"gengxinshijian\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.gengxinshijian\" placeholder=\"更新时间\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item v-if=\"type!='info'\"  label=\"去向说明\" prop=\"quxiangshuoming\" >\r\n\t\t\t\t<editor \r\n\t\t\t\t\tstyle=\"min-width: 200px; max-width: 600px;\"\r\n\t\t\t\t\tv-model=\"ruleForm.quxiangshuoming\" \r\n\t\t\t\t\tclass=\"editor\"\r\n\t\t\t\t\tmyQuillEditor=\"quxiangshuoming\"\r\n\t\t\t\t\taction=\"file/upload\">\r\n\t\t\t\t</editor>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item v-else-if=\"ruleForm.quxiangshuoming\" label=\"去向说明\" prop=\"quxiangshuoming\" >\r\n\t\t\t\t<span class=\"text ql-snow ql-editor\" v-html=\"ruleForm.quxiangshuoming\"></span>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmi