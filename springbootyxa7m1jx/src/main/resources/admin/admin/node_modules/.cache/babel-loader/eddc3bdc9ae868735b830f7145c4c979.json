rm-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"捐赠姓名\" prop=\"juanzengxingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.juanzengxingming\" placeholder=\"捐赠姓名\" clearable  :readonly=\"ro.juanzengxingming\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"捐赠姓名\" prop=\"juanzengxingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.juanzengxingming\" placeholder=\"捐赠姓名\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"性别\" prop=\"xingbie\" >\r\n\t\t\t\t\t<el-select :disabled=\"ro.xingbie\" v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\" >\r\n\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\tv-for=\"(item,index) in xingbieOptions\"\r\n\t\t\t\t\t\t\tv-bind:key=\"index\"\r\n\t\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"性别\" prop=\"xingbie\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.xingbie\"\r\n\t\t\t\t\t\tplaceholder=\"性别\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"年龄\" prop=\"nianling\" >\r\n\t\t\t\t\t<el-input v-model.number=\"ruleForm.nianling\" placeholder=\"年龄\" clearable  :readonly=\"ro.nianling\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"年龄\" prop=\"nianling\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.nianling\" placeholder=\"年龄\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"捐赠手机\" prop=\"juanzengshouji\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.juanzengshouji\" placeholder=\"捐赠手机\" clearable  :readonly=\"ro.juanzengshouji\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"捐赠手机\" prop=\"juanzengshouji\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.juanzengshouji\" placeholder=\"捐赠手机\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-if=\"type!='info' && !ro.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<file-upload\r\n\t\t\t\t\t\ttip=\"点击上传头像\"\r\n\t\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t\t:limit=\"3\"\r\n\t\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t\t:fileUrls=\"ruleForm.touxiang?ruleForm.touxiang:''\"\r\n\t\t\t\t\t\t@change=\"touxiangUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-else-if=\"ruleForm.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<img v-if=\"ruleForm.touxiang.substring(0,4)=='http'&&ruleForm.touxiang.split(',w').length>1\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else-if=\"ruleForm.touxiang.substring(0,4)=='http'\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang.split(',')[0]\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.touxiang.split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t提交\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn4\" v-if=\"type!='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t取消\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn5\" v-if=\"type=='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t返回\r\n\t\t\t\t</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n    \r\n\r\n\t</div>\r\n</template>\r\n<script>\r\n\timport { \r\n\t\tisIntNumer,\r\n\t\tisMobile,\r\n\t} from \"@/utils/validate\";\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\tvar validateMobile = (rule, value, callback) => {\r\n\t\t\t\tif(!value){\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t} else if (!isMobile(value)) {\r\n\t\t\t\t\tcallback(new Error(\"请输入正确的手机号码\"));\r\n\t\t\t\t} else {\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\tvar validateIntNumber = (rule, value, callback) => {\r\n\t\t\t\tif(!value){\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t} else if (!isIntNumer(value)) {\r\n\t\t\t\t\tcallback(new Error(\"请输入整数\"));\r\n\t\t\t\t} else {\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\treturn {\r\n\t\t\t\tid: '',\r\n\t\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\t\tro:{\r\n\t\t\t\t\tjuanzengzhanghao : false,\r\n\t\t\t\t\tmima : false,\r\n\t\t\t\t\tjuanzengxingming : false,\r\n\t\t\t\t\txingbie : false,\r\n\t\t\t\t\tnianling : false,\r\n\t\t\t\t\tjuanzengshouji : false,\r\n\t\t\t\t\ttouxiang : false,\r\n\t\t\t\t},\r\n\t\t\t\r\n\t\t\t\truleForm: {\r\n\t\t\t\t\tjuanzengzhanghao: '',\r\n\t\t\t\t\tmima: '',\r\n\t\t\t\t\tjuanzengxingming: '',\r\n\t\t\t\t\txingbie: '',\r\n\t\t\t\t\tnianling: '',\r\n\t\t\t\t\tjuanzengshouji: '',\r\n\t\t\t\t\ttouxiang: '',\r\n\t\t\t\t},\r\n\t\t\t\txingbieOptions: [],\r\n\r\n\t\t\t\trules: {\r\n\t\t\t\t\tjuanzengzhanghao: [\r\n\t\t\t\t\t\t{ required: true, message: '捐赠账号不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tmima: [\r\n\t\t\t\t\t\t{ required: true, message: '密码不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tjuanzengxingming: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\txingbie: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tnianling: [\r\n\t\t\t\t\t\t{ validator: validateIntNumber, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tjuanzengshouji: [\r\n\t\t\t\t\t\t{ validator: validateMobile, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\ttouxiang: [\r\n\t\t\t\t\t],\r\n\t\t\